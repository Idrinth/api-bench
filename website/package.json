{
  "name": "website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "c8 mocha",
    "serve": "http-server ./dist -c-1 --proxy http://localhost:8080? --silent",
    "e2e": "npm run build && start-server-and-test serve http://localhost:8080 'cypress run'",
    "write-e2e": "cypress open",
    "lint-styles": "stylelint src/index.css src/**/**/*.css"
  },
  "dependencies": {
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-router-dom": "^6.21.1",
    "react-syntax-highlighter": "^15.5.0",
    "the-new-css-reset": "^1.11.2"
  },
  "devDependencies": {
    "@babel/plugin-syntax-import-attributes": "^7.23.3",
    "http-server": "^14.1.1",
    "cypress": "^13.6.2",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "start-server-and-test": "^2.0.3",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "global-jsdom": "^9.2.0",
    "mocha": "^10.2.0",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
